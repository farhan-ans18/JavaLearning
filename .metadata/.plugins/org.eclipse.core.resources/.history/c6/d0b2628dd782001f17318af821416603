package Practice;

import java.util.Scanner;

public class TcsP1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int sum = 0;
		int i = 0;
		while(n>0) {
			if(i%n==0) {
				sum +=i;
				n--;
			}
			i++;
		}
		
		System.out.println(sum);
	}

}
