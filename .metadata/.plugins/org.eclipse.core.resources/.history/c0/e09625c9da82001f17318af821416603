package Practice;

import java.util.Scanner;

public class TcsP1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int sum = 0;
		int i=0;
		int num = 1;
        while(i<49) {
        	if(num%n==0) {
        		sum +=num;
        		i++;
        	}
        	num++;
        }
		
		System.out.println(sum);
	}

}
